 <!-- vseobecne odovzdanie -->
<hr>
<table class="table table-striped table-hover-forum ">
  <thead>
    <tr style="height:45px">
      <th style="text-align: center;">#</th>
      <th style="text-align: center;">Čas odevzdání</th>
      <th>Soubory</th>
      <th>Trofeje</th>
      <th style="text-align: center;">Skóre</th>
    </tr>
  </thead>
  <tbody>
    {{#each model.submissions as |sub|}}
      <tr>
        <td style="text-align: center;" class="numbering"></td>
        <td style="text-align: center;">{{date-and-time sub.datetime 'cs'}}</td>
        <td>{{sub.files}}</td><!-- TODO link na stiahnutie suboru -->
        <td>
          {{#each sub.achievements as |ach|}}
            {{achievement-small model=ach}}
          {{/each}}
        </td>
        <td style="text-align: center;">
          {{#if sub.score}}
            {{sub.score.score}}
          {{else}}
            –
          {{/if}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
