<div class="row" style="margin-left:20px">
	{{{module.description}}}
	{{#if general_error}}
		{{warning-general text=general_error}}
	{{/if}}
</div>
<div class="row" style="margin-left:5px">
	<div class="col-xs-12 col-sm-6">
		<h5>Výsledek:</h5>
		<ul class="sortable list" id="sortable{{id}}a">
			{{#each module.sortable_list.fixed as |line index|}}
				<li draggable="true" class="{{line.style}} static" id="a{{index}}">{{line.content}}</li>
			{{/each}}
		</ul>
	</div>
	<div class="col-xs-12 col-sm-6">
		<h5>Použij tyto řádky:</h5>
		<ul class="sortable list" id="sortable{{id}}b">
			{{#each module.sortable_list.movable as |line index|}}
				<li draggable="true" class="{{line.style}} highlight" id="b{{index}}">{{line.content}}</li>
			{{/each}}
		</ul>
	</div>
</div>
