{{#each model as |org index|}}
    <div class="col-xs-6 col-md-3" style="text-align:center;">
      {{#link-to "user-profile" org}}
          <img src="{{org.profile_picture_r}}" style="
            border-radius: 25px;
            width: 100%;
            margin: 0 auto;
            overflow:hidden;
            position: relative;"
            >
      {{/link-to}}
      {{#link-to "user-profile" org}}
            <div class="org-index-name ">{{org.full_name}}</div>
      {{/link-to}}
    </div>
{{/each}}
